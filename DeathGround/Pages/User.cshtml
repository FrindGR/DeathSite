@page
@model DeathGround.Pages.UserModel
@{
    ViewData["Title"] = "DeathGround";
}

<div class="header">
    <div class="title">
        <img src="/img/png25.png" class="header-logo"/>
        <a class="display" href="/">
            <h1 class="stop-ground">StopGround</h1>
        </a>
    </div>
</div>
<div class="menu">
    <a id="GamesST" class="display-menu" asp-area="" asp-page="/">Игры</a>
    <a id="NewsST" class="display-menu" asp-area="" asp-page="/News">Новости</a>
    <a id="BlogsST" class="display-menu" asp-area="" asp-page="/Blogs">Блоги</a>
    <a id="StreamsST" class="display-menu" asp-area="" asp-page="/Streams">Стримы</a>
    <a id="CheatsST" class="display-menu" asp-area="" asp-page="/Cheats">Читы</a>
    <a id="ForumsST" class="display-menu" asp-area="" asp-page="/Forums">Форумы</a>
</div>
<div class="userblock">
    <div class="fonuserblock">
        <img src="/img/fon2.jpg" class="fonuser"/>
        <div class="blockfotouser">
            <div class="blockfotouser2">
                <img src="/img/foto.jpg" class="fotouser"/>
            </div>
            <div>
                <div class="galochkaright">
                    <h1 class="nickname">Fr1ndGR</h1>
                    <img src="/img/galochka.png" class="galocka" />
                </div>
                <p class="lastonline">Был в сети 7 часов назад</p>
            </div>
        </div>
    </div>
</div>